/* make the overlay work for all iOS devices (iPhone, iPod Touch, iPad) */
@media screen and (max-device-width: 1024px) {
    #simplemodal-overlay {
				width: 1024px !important; /* change to the maximum width of your site */
        height: 5000px !important; /* change to a very large number, greater than the height of the page */
    }
}

#simplemodal-container {
		color:#000;
		background:url(<%= asset_path 'light2.png' %>) top right #c8bfb3 no-repeat;
		border:5px solid #066e9e;
		padding: 0;
		-moz-border-radius:    7px;
		-webkit-border-radius: 7px;
		-khtml-border-radius:  7px;
		border-radius:         7px;
		behavior: url('border-radius.htc');
}

/* all iOS devices (iPhone, iPod Touch, iPad) */
@media screen and (max-device-width: 1024px) {
    #simplemodal-container {
				margin-top: 200px !important;
    }
}

#simplemodal-container .modalCloseImg {
//	display: none;
	background:url(<%= asset_path 'hotelbuttons.png' %>) 0 -481px no-repeat;
	width:30px;
	height:30px;
	display:inline;
	z-index:3200;
	position:absolute;
	top:-12px;
	right:-12px;
	cursor:pointer;
}

#simplemodal-container h3 {
	font:	19px "JosefinSlabBold";
	margin: 10px 0 15px 0;
	padding: 0;
}

#simplemodal-container p {
	margin: 0 0 10px 0;
}

#simplemodal-container button, #simplemodal-container a {
	font: 13px  garamond, times, serif;
	margin: 0 0 0 30px;
}

#simplemodal-container a:hover {
	color: #000;
}

#simplemodal-container #button-container {
	margin: 25px 0 0 95px;
}

#simplemodal-container .simplemodal-data {
	padding:10px;
}




