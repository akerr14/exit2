
/* body */

body {
	background: url(<%= asset_path 'oyster.png' %>) #d9d1c4;
	font: 17px  garamond, times, serif;
	line-height: 22px;
}

/* HEADER */

header.full-width  {   /* full-width is required to override oyster background */
	height: 150px;
  background: url(<%= asset_path 'sublimebg.jpg' %>) center;
}

header .container {
	position: relative;
}


header h1 {
	position: absolute;
	top: 40px;
	left: 0;
  height: 0;
	width: 530px;
	background: url(<%= asset_path 'sublimelogo4.png' %>) no-repeat;
	padding-top:110px;
	font-size: 40px;  /* not displayed */
	overflow: hidden;
}


header h1:hover   {
	background: url(<%= asset_path 'sublimelogo5.png' %>) no-repeat;
}

@media screen and (max-device-width: 1024px) {   /* if change image on hover, it disappears on ipad - so keep the same */
    header h1:hover {
			background: url(<%= asset_path 'sublimelogo4.png' %>) no-repeat;
    }
}

header a, header a:visited, header a:hover {
  color: #eee;
  text-decoration: none;
}

/* NAV  */

nav {
	background: #333;
	border-top: 1px solid #64a4be;
	z-index: 200;
	height: 50px;
}

.BoxGlow_Grey2{       /*   used when nav is fixed at top */
  	        box-shadow: 0 5px 15px rgba(176,176,176, 1);
            -webkit-box-shadow: 0 5px 15px rgba(176,176,176, 1);
            -moz-box-shadow: 0 5px 15px rgba(176,176,176, 1);
}

nav ul {
  font: 14px "HillHouseMedium";
  padding: 0;
  margin: 12px 0 0 0;
	text-align: justify;
}

nav ul li {
  list-style-type: none;
  display: inline;
}

nav a, nav a:visited  {
	vertical-align: 50%;  /* align nav text with lines */
	padding: 6px 10px 3px 10px;
	color: #fff;
	text-decoration: none;
}

nav a:hover {
	background-color: #3a9bc2;
	color: #fff;
	-moz-border-radius:    3px;
  -webkit-border-radius: 3px;
	-khtml-border-radius:  3px;
  border-radius:         3px;
}

nav a.current {         /* identical to hover */
	background-color: #3a9bc2;
	-moz-border-radius:    3px;
  -webkit-border-radius: 3px;
	-khtml-border-radius:  3px;
  border-radius:         3px;
}

nav ul li:first-child a {
	padding-left: 2px;
}

 nav ul #lastnav {
	padding-right: 2px;
}


/*horizontal lines and justifying */

nav ul li span {
	font-size: 1px;      /* need a char for IE to justify - set to a small size */
}

nav ul .line1{
	background-color: #000;
	width: 1px;
	height:25px;
	display: inline-block;
}

nav ul .line2{
	background-color: #fff;
	width: 1px;
	height:25px;
	display: inline-block;
}

nav ul .newline {    /*creates a CR so justify works correctly on the line above */
	visibility: hidden;
	overflow: hidden;
	padding: 0;
	margin:0;
}


